{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy:test": "dotenv -e .env.test -- prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:studio:test": "dotenv -e .env.test -- prisma studio",
    "db:rm": "docker compose rm db -s -f -v",
    "db:up": "docker compose up db -d && sleep 2",
    "db:restart": "yarn db:rm && yarn db:up && yarn prisma:deploy",
    "db:test:rm": "docker compose rm db-test -s -f -v",
    "db:test:up": "docker compose up db-test -d && sleep 2",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && yarn prisma:deploy:test",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "dotenv -e .env.test -- yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --verbose --config ./test/jest-e2e.json",
    "pretest:e2e:watch": "dotenv -e .env.test -- yarn db:test:restart",
    "test:e2e:watch": "dotenv -e .env.test -- jest --verbose --watch --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "9.0.8",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.8",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.8",
    "@nestjs/swagger": "6.0.5",
    "@prisma/client": "4.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.29.1",
    "object-hash": "3.0.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.8",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.5",
    "@types/object-hash": "2.2.1",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "cspell": "6.6.0",
    "dotenv-cli": "6.0.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "markdownlint": "0.26.2",
    "markdownlint-cli": "0.32.1",
    "pactum": "3.1.13",
    "prettier": "2.7.1",
    "prisma": "4.2.0",
    "semantic-release": "19.0.3",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
